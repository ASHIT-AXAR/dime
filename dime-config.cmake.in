set(DIME_VERSION "@PROJECT_VERSION@")
set(DIME_VERSION_MAJOR "@PROJECT_VERSION_MAJOR@")
set(DIME_VERSION_MINOR "@PROJECT_VERSION_MINOR@")
set(DIME_VERSION_PATCH "@PROJECT_VERSION_MICRO@")

@PACKAGE_INIT@

include(GNUInstallDirs)

include("${CMAKE_CURRENT_LIST_DIR}/dime-export.cmake")

if(DIME_BUILD_SHARED_LIBS)
  set(DIME_DEFINITIONS "-DDIME_DLL")
else()
  set(DIME_DEFINITIONS "-DDIME_NOT_DLL")
endif()

set(DIME_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_INCLUDEDIR@")
set(DIME_LIBRARY_DIRS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@")
set(DIME_LIBRARIES "dime::dime")
